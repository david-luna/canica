{
  "name": "canica-electron",
  "productName": "canica-electron",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "dist/index.js",
  "scripts": {
    "build": "ttsc -p tsconfig.build.json",
    "watch": "chokidar \"src/**/*.ts\" -c \"npm run build\"",
    "start-forge": "wait-on http://localhost:3000 && NODE_ENV=development electron-forge start",
    "start-no-ui": "source .env && npm run build && concurrently \"npm run watch\" \"electron-forge start\"",
    "start": "source .env && npm run build && concurrently \"npm run watch\" \"npm run start-forge\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "David Luna",
    "email": "luna.bistuer@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "canica_electron"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/qs": "^6.9.7",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^6.2.0",
    "electron": "12.0.0",
    "eslint": "^7.29.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.20.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.3",
    "ttypescript": "^1.5.12",
    "typescript": "^4.0.2",
    "typescript-transform-paths": "^2.2.4",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "@googleapis/drive": "^0.3.1",
    "@googleapis/sheets": "^0.3.0",
    "annotatron": "^0.0.10",
    "electron-fetch": "^1.7.3",
    "electron-reload": "^1.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "google-auth-library": "^7.3.0",
    "puppeteer-core": "^10.2.0",
    "puppeteer-in-electron": "^3.0.5",
    "qs": "^6.5.2",
    "shallow-equal-object": "^1.1.1",
    "ts-node": "^10.0.0",
    "uuid": "^8.3.2"
  }
}
